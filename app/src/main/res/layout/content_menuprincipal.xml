<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_menuprincipal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"


    android:background="@drawable/nebrascafondopocobrillopng"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.food.sistemas.sodapopapp.Menuprincipal"
    tools:showIn="@layout/app_bar_menuprincipal">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_gravity="bottom|end"
        android:layout_marginEnd="14dp"
        app:backgroundTint="@color/colorprincipal"
        app:srcCompat="@drawable/usrsnack" />
</RelativeLayout>
